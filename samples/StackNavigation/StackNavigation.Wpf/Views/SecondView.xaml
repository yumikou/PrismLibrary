<UserControl x:Class="StackNavigation.Wpf.Views.SecondView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             xmlns:local="clr-namespace:StackNavigation.Wpf.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel>
            <TextBlock FontSize="40">SecondView</TextBlock>
            <TextBlock FontSize="30" Margin="0 20 0 0">
                <Run>StackDepth:</Run>
                <Run Text="{Binding StackDepth}"></Run>
            </TextBlock>
            <Button Command="{Binding GoNextPageCommand}" HorizontalAlignment="Left">GoNewPage</Button>
        </StackPanel>
    </Grid>
</UserControl>
